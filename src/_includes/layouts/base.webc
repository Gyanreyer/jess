<!DOCTYPE html>
<html lang="en" :data-theme="theme">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="description" :content="meta?.description" />
    <meta name="generator" :content="eleventy.generator" />
    <title @html="meta?.title"></title>

    <!-- Critical CSS -->
    <style @raw="getBundle('css', 'critical')" webc:keep></style>
    <link rel="stylesheet" href="../../css/reset.css" webc:bucket="critical" />

    <!-- Critical JS -->
    <script @raw="getBundle('js', 'critical')" type="module" webc:keep></script>

    <!-- Non-critical CSS/JS -->
    <link rel="stylesheet" :href="getBundleFileUrl('css')" webc:keep />
    <script :src="getBundleFileUrl('js')" type="module" webc:keep></script>

    <link rel="stylesheet" href="../../css/page-transition.css" />
    <script src="../../js/page-transition.mjs"></script>
  </head>
  <body data-taxi>
    <template @raw="content" webc:nokeep></template>
    <script
      src="https://unpkg.com/@unseenco/e@2.2.2/dist/e.umd.js"
      crossorigin
      webc:keep
    ></script>
    <script
      src="https://unpkg.com/@unseenco/taxi@1.0.3/dist/taxi.umd.js"
      crossorigin
      webc:keep
    ></script>
    <div id="pg-trns"></div>
  </body>
</html>
